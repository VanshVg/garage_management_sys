<%- include('../partials/head') %>
  <link rel="stylesheet" href="/styles/garageForm.css">
  </head>
  <!-- garage form  -->
  <div class="container">
    <h2 class="heading-main dark">New Garage</h2>
    <form action="#" method="post">
      <div class="tab">
        <h3 class="info-text dark">Garage Information</h3>
        <hr>
        <div class="input-main">
          <div class="input-content">
            <p class="dark input-text">Name</p>
            <input type="text" name="garageName" id="garageName" class="input" tabindex="1">
          </div>
          <div class="input-content">
            <p class="dark input-text">Contact No</p>
            <input type="text" name="contactNumber" id="contactNumber" class="input" tabindex="2">
          </div>
        </div>
        <div class="input-main">
          <div class="input-content">
            <p class="dark input-text">Email</p>
            <input type="text" name="email" id="email" class="input" tabindex="3">
          </div>
          <div class="input-content">
            <p class="dark input-text">Thumbnail</p>
            <input type="text" name="thumbnail" id="thumbnail" class="input" tabindex="4">
          </div>
        </div>
        <div class="input-main">
          <div class="input-content">
            <p class="dark input-text">Open Time</p>
            <input type="text" name="openTime" id="openTime" class="input" tabindex="5">
          </div>
          <div class="input-content">
            <p class="dark input-text">Close Time</p>
            <input type="text" name="closeTime" id="closeTime" class="input" tabindex="6">
          </div>
        </div>
        <div class="input-descripton">
          <div class="input-content">
            <p class="dark input-text desc">Description</p>
            <textarea name="description" id="description" cols="30" rows="10" class="input-description"></textarea tabindex>
          </div>
        </div>
        <input type="button" name="nextButton" class="nextButton" id="nextButton" value="Next" onclick="nextPage()">
      </div>
      <div class="tab">
        <h3 class="info-text dark">Address Information</h3>
        <hr>
        <div class="input-main">
          <div class="input-content">
            <p class="dark input-text">area</p>
            <input type="text" name="area" class="input">
          </div>
          <div class="input-content">
            <p class="dark input-text">State</p>
            <select name="state" id="state" class="input">
              <option value=" " selected></option>
              <option value="gujarat">Gujarat</option>
            </select>
          </div>
        </div>
        <div class="input-main">
          <div class="input-content">
            <p class="dark input-text">City</p>
            <select name="state" id="state" class="input">
              <option value=" " selected></option>
              <option value="gandhinagar">Gandhinagar</option>
            </select>
          </div>
          <div class="input-content">
            <p class="dark input-text">Pincode</p>
            <input type="text" name="pincode" class="input">
          </div>
        </div>
        <div class="upload">
          <img class="upload-image" src="Group 83.svg" alt="upload image">
        </div>
        <div class="button-group">
          <input type="button" name="previousButton" id="previousButton" value="Previous" class="previousButton" onclick="prevPage()">
          <input type="button" name="add" id="addGarage" value="+ Add" class="addGarage" >
        </div>
        
      </div>
    </form>
  </div>
  <script src="/scripts/garageForm.js"></script>
  <%- include('../partials/foot') %>