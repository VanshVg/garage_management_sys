
<div class="grid-slot flex items-center justify-center w-full" id="simpleCalendar">
    <div class="calendar-container">
      <header class="calendar-header">
        <p class="calendar-current-date" id="month"></p>
        <div class="calendar-navigation">
          <span id="calendar-prev" class="material-symbols-rounded">
            <img src="/icons/left.svg" alt="&lt;">
          </span>
          <span id="calendar-next" class="material-symbols-rounded">
            <img src="/icons/right.svg" alt="&gt;">
          </span>
        </div>
      </header>

      <div class="calendar-body">
        <ul class="calendar-weekdays">
          <li>Sun</li>
          <li>Mon</li>
          <li>Tue</li>
          <li>Wed</li>
          <li>Thu</li>
          <li>Fri</li>
          <li>Sat</li>
        </ul>
        <ul class="calendar-dates"></ul>
      </div>
    </div>

    <div class="slot-listing" id="slot-listing" style="display: none;">
      <table id="slot-table">
        <thead>
          <tr>
            <th>Slot</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody id="slot-body">

        </tbody>
      </table>
    </div>
</div>