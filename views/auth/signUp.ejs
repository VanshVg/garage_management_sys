<%- include('../partials/head.ejs') %>
  <link rel="stylesheet" href="/styles/auth.css">
  </head>

  <body>
    <div class="section">
      <div class="container">
        <div class="logo">
          <img src="logo.png" alt="logo page">
        </div>
        <p class="projectTitle">Garage Management System</p>
        <div class="imgContainer">
          <img src="Group 35.png" alt="car image">
          <img src="Group 15.png" alt="owner image">
        </div>
        <form action="#">
          <div class="formContainer">
            <div class="pageInputs">
              <div class="pageIcons">
                <img class="icons"
                  src="data:image/png;base64," />
              </div>

              <input type="text" name="name" id="name" placeholder="your name">

            </div>
            <div class="formContainer">
              <div class="pageInputs">
                <div class="pageIcons">
                  <img class="icons"
                    src="data:image/png;base64," />
                </div>

                <input type="text" name="email" id="email" placeholder="abc@pqr.xyz">

              </div>
              <div class="pageInputs">
                <div class="pageIcons">
                  <img class="icons"
                    src="data:image/png;base64," />
                </div>

                <input type="password" name="password" id="password" placeholder="* * * * * * * *">

              </div>

              <div class="pageInputs">
                <div class="pageIcons">
                  <img class="icons"
                    src="data:image/png;base64," />
                </div>

                <input type="password" name="conformPassword" id="conformPassword" placeholder="* * * * * * * *">

              </div>

              <input type="button" class="registerButton" id="registerButton" value="REGISTER">
              <div class="createAccount">
                already have account ? <a id="registerLogin" href="/signin">login</a>
              </div>
        </form>
      </div>
    </div>
    <%- include('../partials/foot.ejs') %>